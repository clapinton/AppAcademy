<tr>
  <td><%= req.start_date %></td>
  <td><%= req.end_date %></td>
  <td>
    <% if owner && req.pending? %>
        <%= button_to "Approve", approve_cat_rental_request_url(req) %>
        <%= button_to "Deny", deny_cat_rental_request_url(req) %>
    <% else %>
      <% if req.status == "APPROVED" %>
      <%= req.renter.user_name %>
      <% else %>
      <%= req.status %>
      <% end %>
    <% end %>
  </td>
</tr>
